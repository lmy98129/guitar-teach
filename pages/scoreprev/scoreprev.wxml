<top-widget title="{{title}}">
  <navi-btn slot="back-btn" btn-style="back-btn" bind:tapbtn="bindBack" insert="true"/>
</top-widget>
<view class="root">
  <image src="/images/bg.jpeg" class="upload-bg"></image>
    <scroll-view scroll-y="true" scroll-x="false" style="height: 60%;" scroll-top="{{scrollTop}}">
      <view class="input-wrapper">
          <view class="key-wrapper" wx:for="{{keyArray}}" wx:key="{{index}}">
            <text class="tune" decode="true">{{item.tune}}</text>
            <text class="key {{item.style == 'High'?'high':''}} {{item.style == 'Low'?'low':''}}" decode="true">{{item.key}}</text>
          </view>
      </view>
    </scroll-view>
</view>